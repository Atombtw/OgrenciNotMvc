@model OgrenciNotMvc.Models.Entity.Tbl_Kulüpler
@{
    ViewBag.Title = "Güncelle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form method="post">
    @Html.Label("Kulüp Ad")
    @Html.TextBoxFor(x => x.KulüpAd, new { @class = "form-control" })
    @Html.Label("Kontenjan")
    @Html.TextBoxFor(x => x.KulüpKontenjan, new { @class = "form-control" })
    <br />
    @Html.HiddenFor(x => x.KulüpID)
    <button class="btn btn-primary">Güncelle</button>
</form>